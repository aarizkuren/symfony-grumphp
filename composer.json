{
    "name": "sci3ma/sf4grumphp",
    "description": "Configured GrumPHP with bunch of tools for static code analysis Symfony Framework v4.",
    "keywords": ["php", "symfony", "grumphp", "phpcs", "phpmd", "phpstan", "phpunit"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcin Harasim",
            "email": "marcin.harasim@gmail.com"
        }
    ],
    "type": "library",
    "require": {
		"php": ">=7.1",
        "composer/composer": "^1.8",
        "friendsofphp/php-cs-fixer": "^2.14",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "phpro/grumphp": "^0.15.0",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan": "^0.11.2",
        "phpstan/phpstan-doctrine": "^0.11.1",
        "phpstan/phpstan-phpunit": "^0.11.0",
        "phpstan/phpstan-symfony": "^0.11.1",
        "phpunit/phpunit": "^6.5|^7.0",
        "povils/phpmnd": "^2.1",
        "sebastian/phpcpd": "^4.1",
        "sensiolabs/security-checker": "^5.0",
        "symfony/console": "^4.2",
        "symfony/filesystem": "^4.2"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "MH\\": "src/"
        }
    },
    "support": {
        "issues": "https://github.com/sci3ma/sf4grumphp/issues"
    },
    "homepage": "https://github.com/sci3ma/sf4grumphp",
    "config": {
        "sort-packages": true
    },
    "bin": ["bin/sf4grumphp"]
}
